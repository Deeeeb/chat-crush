{
    "manifest_version": 3,
    "name": "Chat to crush",
    "version": "1.0.0",
    "description": "A simple extension that helps you 1st move to your crush.",
    "action": {
      "default_popup": "index.html"
    },
    "permissions": ["activeTab", "scripting", "tabs"],
    "host_permissions": ["<all_urls>"]
  }